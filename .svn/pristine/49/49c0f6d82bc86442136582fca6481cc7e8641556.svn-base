package banking.bean;
public class Compte{
    private int idCompte;
    private double solde;
    private double decouvert;
    private double debitMax;
    private String nomProp;
    private String prenomProp;
    private String adresseProp;
    private int idProp;

    public double getSolde() {
        return solde;
    }

    public void setSolde(double solde) {
        this.solde = solde;
    }

    public double getDecouvert() {
        return decouvert;
    }

    public void setDecouvert(double decouvert) {
        this.decouvert = decouvert;
    }

    public double getDebitMax() {
        return debitMax;
    }

    public String getNomProp() {
        return nomProp;
    }

    public void setNomProp(String nomProp) {
        this.nomProp = nomProp;
    }

    public String getPrenomProp() {
        return prenomProp;
    }

    public void setPrenomProp(String prenomProp) {
        this.prenomProp = prenomProp;
    }

    public String getAdresseProp() {
        return adresseProp;
    }

    public void setAdresseProp(String adresseProp) {
        this.adresseProp = adresseProp;
    }

    public int getIdProp() {
        return idProp;
    }

    public void setIdProp(int idProp) {
        this.idProp = idProp;
    }
    
    
    public void setDebitMax(double debitMax) {
        this.debitMax = debitMax;
    }

    public void setIdCompte(int idCompte) {
        this.idCompte = idCompte;
    }

    public int getIdCompte() {
        return idCompte;
    }
    
    //Constrcuteur
    public Compte(double solde, double decouvert, double debitMaxe, String nom,String prenom,String adresse) {
        this.solde = solde;
        this.decouvert = decouvert;
        this.debitMax = debitMaxe;
        this.nomProp = nom;
        this.prenomProp = prenom;
        this.adresseProp = adresse;
    }

    public Compte(double solde, double decouvert, double debitMax) {
        this.solde = solde;
        this.decouvert = decouvert;
        this.debitMax = debitMax;
    }
    
    
     public Compte() {
         
    }
}

